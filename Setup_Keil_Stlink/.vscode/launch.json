{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug STM32",
      "cwd": "${workspaceFolder}",
      "executable": "${workspaceFolder}/Objects/pp.hex",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "openocd",
      "device": "STM32F446RE",
      "configFiles": [
        "interface/stlink.cfg",
        "target/stm32f4x.cfg"
      ],
      "runToEntryPoint": "main",
      "preLaunchTask": "Build",
      "svdFile": "${workspaceFolder}/STM32F446.svd",
      "showDevDebugOutput": "none"
    }
  ]
}
